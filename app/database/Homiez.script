CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE SEQUENCE USERSEQ AS INTEGER START WITH 0 
CREATE SEQUENCE POSTINGSEQ AS INTEGER START WITH 0 
CREATE MEMORY TABLE USERS(USERID VARCHAR(50) NOT NULL PRIMARY KEY,NAME VARCHAR(60) NOT NULL,AGE INTEGER NOT NULL,GENDER CHAR,BUDGET DOUBLE NOT NULL,DESCRIPTION VARCHAR(512))
CREATE MEMORY TABLE POSTINGS(POSTINGID VARCHAR(50) NOT NULL PRIMARY KEY,USERID VARCHAR(50) NOT NULL,TITLE VARCHAR(100),PRICE DOUBLE NOT NULL,LOCATION VARCHAR(60) NOT NULL,TYPE VARCHAR(50) NOT NULL,DESCRIPTION VARCHAR(512),CONSTRAINT SYS_FK_48 FOREIGN KEY(USERID) REFERENCES USERS(USERID))
CREATE MEMORY TABLE USERPOSTINGS(USERID VARCHAR(50) NOT NULL,POSTINGID VARCHAR(50) NOT NULL,PRIMARY KEY(USERID,POSTINGID),CONSTRAINT SYS_FK_53 FOREIGN KEY(USERID) REFERENCES USERS(USERID),CONSTRAINT SYS_FK_54 FOREIGN KEY(POSTINGID) REFERENCES POSTINGS(POSTINGID))
CREATE MEMORY TABLE ATTACHEDUSERS(POSTINGID VARCHAR(50) NOT NULL,USERID VARCHAR(50) NOT NULL,PRIMARY KEY(POSTINGID,USERID),CONSTRAINT SYS_FK_61 FOREIGN KEY(USERID) REFERENCES USERS(USERID),CONSTRAINT SYS_FK_62 FOREIGN KEY(POSTINGID) REFERENCES POSTINGS(POSTINGID))
CREATE MEMORY TABLE REQUESTS(USERID VARCHAR(50) NOT NULL,POSTINGID VARCHAR(50) NOT NULL,PRIMARY KEY(USERID,POSTINGID),CONSTRAINT SYS_FK_69 FOREIGN KEY(USERID) REFERENCES USERS(USERID),CONSTRAINT SYS_FK_70 FOREIGN KEY(POSTINGID) REFERENCES POSTINGS(POSTINGID))
CREATE MEMORY TABLE MATCHES(USERID VARCHAR(50) NOT NULL,POSTINGID VARCHAR(50) NOT NULL,PRIMARY KEY(USERID,POSTINGID),CONSTRAINT SYS_FK_77 FOREIGN KEY(USERID) REFERENCES USERS(USERID),CONSTRAINT SYS_FK_78 FOREIGN KEY(POSTINGID) REFERENCES POSTINGS(POSTINGID))
CREATE MEMORY TABLE INTERESTS(INTEREST VARCHAR(60), USERID VARCHAR(50) NOT NULL, PRIMARY KEY(INTEREST,USERID), FOREIGN KEY(USERID) REFERENCES USERS(USERID))
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
